[DEFAULT]
# OpenAI API KEY
openai_api_key = YOUR_API_KEY
# Google API KEY
google_api_key = YOUR_API_KEY
# LLM to be used 'gpt-4' or palm-2
model = gpt-4
# model temperature
temp = 0.75
# number of attempts/samples
K_run = 5
# top_p (0,1) default: 1
top_p = 0.9
# Adjustable number of tokens to control response length
max_tokens = 4096
# Number of completions to generate
n = 1 
# Stop completion tokens
stop = None
# waiting time
cool_down_time = 3
# prompt path (absolute path)
prompt_path = path_to_prompt
# base output path (absolute path)
base_output_path = path_to_output_for_generated_code
